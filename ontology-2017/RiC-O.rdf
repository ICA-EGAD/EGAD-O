        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentCharacteristic -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentCharacteristic">
