            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#agentControlHeldBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#controlOnAgent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">onnects any kind of agent and another one, which is somewhat controlled by it.
Example : the ministry of higher education controlling a university</rdfs:comment>
        <rdfs:label xml:lang="en">Control Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentHierarchicalRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentHierarchicalRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#SocialRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#hasHierarchicalChild"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#hasHierarchicalParent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The same vague relation as in EAC-CPF</rdfs:comment>
        <rdfs:label xml:lang="en">Agent Hierarchical Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentLeadershipRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentLeadershipRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#AgentControlRelation"/>
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#AgentMembershipRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#leadershipBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#leadershipOn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#SocialAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#leadershipWithPosition"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Position"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The relation between a person and the group he/she leads.</rdfs:comment>
        <rdfs:label xml:lang="en">Agent leadership relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentMembershipRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentMembershipRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#MembershipRelation"/>
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#SocialRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#agentMembershipIn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Group"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#hasAgentMember"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#membershipWithPosition"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Position"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Simply connects an agent and a group of which he/she/it is a member, without specifying what kind of membership (affiliation, formal arrangement...)
Possibly related to the position that the agent occupies within the group of which it is a member</rdfs:comment>
        <rdfs:label xml:lang="en">Agent Membership Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentName -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentName">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#AgentCharacteristic"/>
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Name"/>
        <rdfs:label xml:lang="en">Agent Name</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentNameType -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentNameType">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Type"/>
        <rdfs:comment xml:lang="en">e.g. : pseudonym, patronym, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Agent Name Type</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentPhysicalTrait -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentPhysicalTrait">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Characteristic-Physical"/>
        <rdfs:label xml:lang="en">Agent Physical Trait</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentProductionRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentProductionRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#RecordsProductionRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#producingEntity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Agent Production Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentTemporalRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentTemporalRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#TemporalRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#asConcerns"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#followingAgent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#precedingAgent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The relation exiting between two agents when agent B replaces agent A for fullfilling some functions or performing some activities. Agent B may result from a significant change of Agent A.</rdfs:comment>
        <rdfs:label xml:lang="en">Agent Succession Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentWholePartRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentWholePartRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#AgentHierarchicalRelation"/>
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#WholePartRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#agentPartOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Group"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#wholeHasAgentPart"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Group"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Implies that one agent is a part of (a subdivision of) a group</rdfs:comment>
        <rdfs:label xml:lang="en">Agent Whole Part Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AuthorityRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#ArchivalProvenanceRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Relation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#originatedObject"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#Record"/>
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#RecordElement"/>
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#RecordSet"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#provenanceEntity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#Activity"/>
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#Agent"/>
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#Occupation"/>
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#Position"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#inFullfilmentOfFunction"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Function"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The quite vague relation that connects an agent, an cccupation, a position, an activity, and the record(s) and record set(s) it makes, accumulates or maintains and uses in the course of its activities. Key relation in the archival world. Can also involve a function. Subclasses are accumulation relation and creation one.
See ISAD(G) and Interpares glossary</rdfs:comment>
        <rdfs:label xml:lang="en">Archival Provenance Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AuthorityRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AuthorityRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Relation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#authorityHeldBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#authorityOn"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The relation between an agent and the things on which it has some power or authority</rdfs:comment>
        <rdfs:label xml:lang="en">Authority Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AuthorizationRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AuthorizationRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Relation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#authorizationDefinedBy"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#authorizationGivenTo"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A relation that connects one mandate and one agent, to which the mandate delegates some authority, or whose authority and organization it changes, or whose existence it ends, at or for some time, possibly in some location. May also involve some some Function, some Activity, Position or Occupation.</rdfs:comment>
        <rdfs:label xml:lang="en">Authorization Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AuthorshipRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AuthorshipRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Relation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#authorOf"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#authorshipBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#authorshipWithRole"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The relation between an author -in a bibliographic meaning, as well as in diplomatics - and an object. The author agent is intellectually responsible of conceptualizing the object, of its meaning and content (example : a person being the author of a work, of a software). The agent may also be responsible of issuing the object (if it is a record), by itself or through another agent that acts by its name, command or delegation. Not to be confused with the agent production relation.
QUESTION : do we introduce a Role class here, for specifying the agent&apos;s role in this relation ? I did it, but want to know what you think of this.</rdfs:comment>
        <rdfs:label xml:lang="en">Authorship Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#BusinessRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#BusinessRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#SocialRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#businessRelationWith"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ica.org/standards/RiC/ontology#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A relation that exists between two agents which work together, for example in the course of a business process for fullfilling a function. &quot;works with&quot; relation.</rdfs:comment>
        <rdfs:label xml:lang="en">Business Relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#Characteristic -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#Characteristic">
        <rdfs:comment xml:lang="en">Characteristic (social or cultural), that is inherent to an object. Cannot exist if the object does not exist.
See DUL Quality class
QUESTION: do we introduce a Region class and the hasRegiondatavalue data property ?</rdfs:comment>
        <rdfs:label xml:lang="en">Characteristic</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#Characteristic-Physical -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#Characteristic-Physical">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Characteristic"/>
        <rdfs:comment>Do we distinguish physical traits from social, cutural ones? 
See DUL Quality class
QUESTION: do we also introduce a Region class, with possible subclasses like PhysicalAttribute, and the hasRegiondatavalue data property ?</rdfs:comment>
        <rdfs:label xml:lang="en">Characteristic (physical)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#Characteristic-SocialOrCultural -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#Characteristic-SocialOrCultural">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#Characteristic"/>
        <rdfs:label xml:lang="en"></rdfs:label>
    </owl:Class>
    


