                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#Record"/>
                            <rdf:Description rdf:about="http://www.ica.org/standards/RiC/ontology#RecordSet"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ica.org/standards/RiC/ontology#recordSetType"/>
                <owl:allValuesFrom rdf:resource="http://www.ica.org/standards/RiC/ontology#RecordSetType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Record Set</rdfs:label>
    </owl:Class>
    


