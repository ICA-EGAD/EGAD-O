        <rdfs:comment xml:lang="en">See DUL Quality</rdfs:comment>
        <rdfs:label xml:lang="en">Agent Characteristic</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ica.org/standards/RiC/ontology#AgentControlRelation -->

    <owl:Class rdf:about="http://www.ica.org/standards/RiC/ontology#AgentControlRelation">
        <rdfs:subClassOf rdf:resource="http://www.ica.org/standards/RiC/ontology#AgentHierarchicalRelation"/>
